<p id="notice"><%= notice %></p>

<h1>Lists</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>
  <td><%= link_to 'Show All', board_list_path %></td>
  <%= link_to 'New List', new_board_list_path %>

  <tbody>
    <% @lists.each do |list| %>
      <tr>
        <td><%= link_to 'Edit', edit_board_list_path(list) %></td>
        <td><%= link_to 'Delete', list, method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

